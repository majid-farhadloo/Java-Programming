import java.util.Scanner;

public class MyInteger {
	static int value;

	public MyInteger(int object){
		value= object;
	}
	public int getValue() {
		return value;
	}
	public static boolean isEven(){
		if(value%2==0){
			return true;
		}
		return false;
	}
	public static boolean isOdd(){
		if(value%2==0){
			return false;
		}
		return true;
	}
	public static boolean isPrime(){
		for(int i=2;i<value;i++){
			if(value% i==0){
				return false;
			}
		}
		return true;
	}
	public static boolean equals(int val1){
		if( value == val1){
			return true;	
		}
		return false;
	}
	public static boolean isEven(int val){
		if(val%2==0){
			return true;
		}
		return false;
	}
	public static boolean isOdd(int val){
		if(val%2==0){
			return false;
		}
		return true;
	}
	public static boolean isPrime(int value){
		for(int i=2;i<value;i++){
			if(value% i==0){
				return false;
			}
		}
		return true;
	}

	public static boolean equals(MyInteger myInt){
		if( value == myInt.value){
			return true;	
		}
		return false;
	}
	public static int parseInt(char[] numChar){
		double value  = 0;
		for(int i = 0; i<numChar.length;i++){
			value += (numChar[i] - 48)*Math.pow(10,numChar.length - i-1);
		}
		return (int) value;
	}
	public static int parseInt(String str){
		int value = Integer.parseInt(str);
		return value;

	}
}


